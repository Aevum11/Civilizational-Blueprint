        with np.errstate(divide='ignore', invalid='ignore'):
            t_act = ETDyingWorldAnalyzer.GRAD_D / v
        t_act[np.isnan(t_act)] = 0.0  # Resolve [0/0] to 0 (inert T, ET limit)
